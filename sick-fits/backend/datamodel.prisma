# This file is for setting up the backend with prisma that generates the prisma.graphql file
# EVERY TIME YOU MAKE CHANGES TO THE DATAMODEL=> REDEPLOY ON PRISMA FOR UPDATES TO TAKE HOLD
type User {
  id: ID! @unique
  name: String!
  email: String!
}

type Item {
  id: ID! @unique
  title: String!
  description: String!
  image: String!
  largeImage: String
  price: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
  # user: User!
}